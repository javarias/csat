ACSErr::Completion_var c=new ACSErr::Completion;


/***************************
 * FILL IN HERE YOUR CODE! *
 ***************************
*/
if(DO_running)
{
	if(vidDev->deactivate()>=0)
	{
		DO_running=false;
		if(vidDev->activate()>=0)
		{
			c->type=ACSErr::ACSErrTypeOK;
			c->code=ACSErrTypeOK::ACSErrOK;
			DO_running=true;
			ACS_SHORT_LOG((LM_INFO,"hptWebCam::resetFunct: Camera is activated."));
		}
		else
		{
			c->type=ACSErr::ACSErrTypeCommon;
			c->code=ACSErrTypeCommon::IOError;
			DO_running=false;
			ACS_SHORT_LOG((LM_ERROR,"hptWebCam::resetFunct: Camera is NOT activated."));
		}
	}
	else
	{
		c->type=ACSErr::ACSErrTypeCommon;
		c->code=ACSErrTypeCommon::IOError;
		DO_running=false;
		ACS_SHORT_LOG((LM_ERROR,"hptWebCam::onFunct: Camera is NOT deactivated."));
	}
}
else
{
	c->type=ACSErr::ACSErrTypeCommon;
	c->code=ACSErrTypeCommon::IOError;
	ACS_SHORT_LOG((LM_ERROR,"hptWebCam::onFunct: Cannot start component. Check if component is already running."));
}
/**************************
 * UP TO HERE!            *
 **************************
*/

c->timeStamp=getTimeStamp();
/*
 * The ACSErr::Completion is set here for safety,
 * please return the proper values!!!
 *
*/
c->type=ACSErr::ACSErrTypeOK;
c->code=ACSErr::ACSErrOK;

return c._retn();
