ACSErr::Completion_var c;
ACS::longSeq *temp;
if(use_network_compression!=use_compression)
{
	use_network_compression=use_compression;
	frameIO_p->use_network_compression(use_compression);
}
temp=m_frame_p->get_sync(c.out());

if(c->code!=0)
{
	ACS_SHORT_LOG((LM_ERROR,"hptWebCam::getFrameFunct: No frame arrived. Completion type=%d, code=%d.",c->type,c->code));
}

return temp;
