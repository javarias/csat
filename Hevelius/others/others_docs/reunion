- void gotoAltAz(AltAzPos p, AltAzVel v); A
- void AltitudeOffSet(double degree); S
- void AzimuthOffSet(double degree); S
- void getPreviewImage(Image); A
- void StopTelescope(); S
- void getProImage(int id, double exptime); A




STATUS:
- void on(); S
- void stop(); A
- void off(); S
- void setUncalibrated(); S
- void setCalibrated(AltAzPos); S
- void initialize(); A
- void getPos(RadecPos r, AltAz a); S
- int getState(); S
- bool getTrackingStatus(); S
- RadecVel getTrackingRate(); S
- int getSafety(RadecPos p); S
- void EmergencyStop(); S
- void setMode(int); S






